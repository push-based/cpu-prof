{
  "name": "@push-based/cpu-profiling",
  "version": "0.0.1",
  "type": "commonjs",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.cjs.js"
    }
  },
  "bin": {
    "@push-based/cpu-prof": "./dist/cpu-prof.cjs.js"
  },
  "scripts": {},
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "dependencies": {
    "yargs": "^17.7.2"
  },
  "nx": {
    "name": "cpu-profiling",
    "targets": {
      "unit-test": {
        "executor": "@nx/vite:test",
        "options": {
          "config": "packages/{projectName}/vitest.unit.config.ts"
        }
      },
      "integration-test": {
        "executor": "@nx/vite:test",
        "options": {
          "config": "packages/{projectName}/vitest.integration.config.ts"
        }
      },
      "measure": {
        "command": "node --cpu-prof --cpu-prof-dir profiles"
      },
      "cpu-prof-cli": {
        "dependsOn": [
          "build"
        ],
        "executor": "nx:run-commands",
        "options": {
          "command": "node packages/cpu-profiling/dist/profiling.esm.js --help"
        },
        "configurations": {
          "help": {
            "command": "node packages/cpu-profiling/dist/profiling.esm.js --help"
          },
          "reduce": {
            "command": "node packages/cpu-profiling/dist/profiling.esm.js trace-reduce"
          },
          "cpu-merge": {
            "command": "node packages/cpu-profiling/dist/profiling.esm.js cpu-merge"
          }
        }
      }
    },
    "tags": [
      "package"
    ]
  }
}
