{
  "name": "@push-based/cpu-profiling",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.esm.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.esm.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.esm.d.ts",
      "import": "./dist/index.esm.js",
      "default": "./dist/index.esm.js"
    }
  },
  "bin": {
    "@push-based/cpu-profiling": "./dist/profiling.esm.js"
  },
  "scripts": {

  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "dependencies": {},
  "nx": {
    "name": "cpu-profiling",
    "targets": {
      "measure": {
        "command": "node --cpu-prof --cpu-prof-dir profiles"
      },
      "merge": {
        "dependsOn": ["build"],
        "command": "node packages/cpu-profiling/dist/profiling.esm.js cpu-merge --cpu-profiles"
      },
      "cpu-prof-cli": {
        "dependsOn": ["build"],
        "executor": "nx:run-commands",
        "options": {
          "command": "node packages/cpu-profiling/dist/profiling.esm.js --help"
        },
        "configurations": {
          "help": {
            "command": "node packages/cpu-profiling/dist/profiling.esm.js --help"
          },
          "reduce": {
            "command": "node packages/cpu-profiling/dist/profiling.esm.js trace-reduce"
          },
          "cpu-merg": {
            "command": "node packages/cpu-profiling/dist/profiling.esm.js cpu-merge"
          }
        }
      }
    },
    "tags": ["package"]
  }
}
