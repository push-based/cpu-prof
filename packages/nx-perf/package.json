{
  "name": "@push-based/nx-perf",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.esm.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.esm.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.esm.d.ts",
      "import": "./dist/index.esm.js",
      "default": "./dist/index.esm.js"
    }
  },
  "bin": {
    "@push-based/nx-perf": "./dist/bin.esm.js"
  },
  "scripts": {
    "postinstall": "node ./dist/postinstall.esm.js"
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "dependencies": {},
  "nx": {
    "targets": {
      "measure": {
        "dependsOn": ["build"],
        "command": "node packages/nx-perf/dist/bin.esm.js -v"
      }
    }
  }
}
